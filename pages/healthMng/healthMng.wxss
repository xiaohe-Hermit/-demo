.container{
  flex-direction: column;
}
.topTap{
  display: flex;
  height: 100rpx;
  border-bottom: rgba(242, 244, 245) solid 5rpx;
}
.tap1{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  margin-left: 120rpx;
  color: gray;
}
.tap1Selected{
display: flex;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  margin-left: 120rpx;
  color: rgb(251, 0, 255);
  border-bottom: rgb(251, 0, 255) solid 6rpx;
}
.tap2{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  margin-left: 150rpx;
  color: gray;
}
.tap2Selected{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  margin-left: 150rpx;
  color: rgb(251, 0, 255);
  border-bottom: rgb(251, 0, 255) solid 6rpx;
}
.result{
  display: flex;
  width: 90%;
  height: 200rpx;
  margin-left: 5%;
  margin-top: 20rpx;
  border-bottom: rgba(242, 244, 245) solid 5rpx;
}
.logo_clock{
  display: flex;
  background-color: rgba(255, 236, 233, 1);
  height: 150rpx;
  width: 150rpx;
  border-radius: 30rpx;
  margin-top: 20rpx;
  align-items: center;
  justify-content: center;
}
.logo_data{
  display: flex;
  background-color: rgba(209, 232, 158, 1);
  height: 150rpx;
  width: 150rpx;
  border-radius: 30rpx;
  margin-top: 20rpx;
  align-items: center;
  justify-content: center;
}
.logoImage{
  height: 80rpx;
  width: 80rpx;
}
.result_content{
  display: flex;
  flex-direction: column;
  /* background-color: green; */
  margin-left: 30rpx;
  margin-top: 40rpx;
  height: 150rpx;
  width: 250rpx;
}
.result_type{
    font-size: 35rpx;
}
.result_date{
    font-size: 30rpx;
    color: rgb(155, 155, 155);
}
.like_logo{
    display: flex;
    height: 75rpx;
    width: 75rpx;
    border-radius: 50%;
    align-items: center;
    justify-content: center;
    margin-top: 40rpx;
    margin-left: 150rpx;
    border: rgba(242, 244, 245, 1) solid 5rpx;
}
.like{
    margin-top: 10rpx;
    width: 45rpx;
    height: 45rpx;
}
.like2{
    margin-top: 2rpx;
    width: 45rpx;
    height: 45rpx;
}
.missing{
  display: flex;
  color: gray;
  height: 100%;
  justify-content: center;
}
.missing text{
  margin-top: 100rpx;
}
